{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Mvp24Hours": {
    "Web": {
      "TraceMiddleware": true,
      "ResponseCompressionForHttps": false
    },
    "Filters": {
      "EnableNotification": true,
      "EnableHateoas": false
    }
  }
}
